{
  "name": "react-starter-kit",
  "version": "0.0.0",
  "description": "React Starter Kit",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --host 0.0.0.0 --env.NODE_ENV 'dev' --env.BUILD_ENV 'dev'",
    "build:dev": "npm run clean-build-dir && webpack --env.NODE_ENV 'dev' --env.BUILD_ENV 'optimized'",
    "build:prod": "npm run clean-build-dir && webpack --env.NODE_ENV 'prod' --env.BUILD_ENV 'optimized'",
    "deploy": "echo \"Error: no deploy process specified\" && exit 1", 
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean-build-dir": "rm -rf ./public && mkdir ./public"
  },
  "author": "James Friedman",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.0",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.3",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.6"
  }
}
